<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_questions"
    app:startDestination="@id/questionsFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/questionsFragment"
        android:name="com.can_apps.questions.app.QuestionsFragment"
        android:label="fragment_questions"
        tools:layout="@layout/fragment_questions" >
        <action
            android:id="@+id/actionShowResults"
            app:destination="@id/resultFragment" />
    </fragment>
    <fragment
        android:id="@+id/resultFragment"
        android:name="com.can_apps.questions.app.ResultFragment"
        android:label="ResultFragment" />
</navigation>